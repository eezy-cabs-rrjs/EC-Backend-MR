{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "analytics": {
      "type": "application",
      "root": "apps/analytics",
      "entryFile": "main",
      "sourceRoot": "apps/analytics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "booking": {
      "type": "application",
      "root": "apps/booking",
      "entryFile": "main",
      "sourceRoot": "apps/booking/src",
      "compilerOptions": {
        "tsConfigPath": "apps/booking/tsconfig.app.json"
      }
    },
    "chat": {
      "type": "application",
      "root": "apps/chat",
      "entryFile": "main",
      "sourceRoot": "apps/chat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat/tsconfig.app.json"
      }
    },
    "drivers": {
      "type": "application",
      "root": "apps/drivers",
      "entryFile": "main",
      "sourceRoot": "apps/drivers/src",
      "compilerOptions": {
        "tsConfigPath": "apps/drivers/tsconfig.app.json"
      }
    },
    "navigation": {
      "type": "application",
      "root": "apps/navigation",
      "entryFile": "main",
      "sourceRoot": "apps/navigation/src",
      "compilerOptions": {
        "tsConfigPath": "apps/navigation/tsconfig.app.json"
      }
    },
    "payment": {
      "type": "application",
      "root": "apps/payment",
      "entryFile": "main",
      "sourceRoot": "apps/payment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "verification": {
      "type": "application",
      "root": "apps/verification",
      "entryFile": "main",
      "sourceRoot": "apps/verification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/verification/tsconfig.app.json"
      }
    }
  }
}